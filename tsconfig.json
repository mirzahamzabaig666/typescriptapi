{
  "compilerOptions": {
    "lib": [
      "esnext.asynciterable",
      "es5",
      "es6"
    ],
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": false,
    "sourceMap": true
  }
}
